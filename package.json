{
  "name": "seo-console-package-workspace",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/demo",
    "build": "npm run build --workspace=packages/seo-console && npm run build --workspace=apps/demo",
    "build:package": "npm run build --workspace=packages/seo-console",
    "start": "npm run start --workspace=apps/demo",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "typecheck": "npm run typecheck --workspace=packages/seo-console && npm run typecheck --workspace=apps/demo",
    "test": "node scripts/test.js",
    "verify": "npx latticeai verify",
    "format": "prettier --write .",
    "test:e2e": "playwright test",
    "release-check": "npx latticeai release-check",
    "e2e": "echo \"Configure e2e tests (e.g., playwright or cypress)\"",
    "security": "echo \"Configure security scanning\"",
    "audit": "npm audit --audit-level=high || true"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-form": "^0.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@supabase/ssr": "^0.6.0",
    "@supabase/supabase-js": "^2.47.0",
    "cheerio": "^1.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.468.0",
    "next": "^16.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.54.2",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^4.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@eslint/eslintrc": "^3.0.0",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.49.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "turbo": "^2.3.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^2.1.8"
  }
}
